function _docker_create()
{
    local _TAG=${1}

    case "${_TAG}" in
        darr|DARR)
            local _SVCS_DARR="header.service
            sabnzbd.service
            nzbhydra2.service
            radarr.service
            lidarr.service
            sonarr.service
            lazylibrarian.service
            ubooquity.service
            plex.service
            tautulli.service
            network.service"

            if [ ! -d "${_CFG_PATH}/" ]
            then
                mkdir ${_CFG_PATH}
            fi

            for ITER in ${_SVCS_DARR}
            do
                cat ${_SVC_PATH}/${ITER} \
                    >> ${_CFG_PATH}/${_YML_FILE}
            done

            cat ${_DKR_PATH}/.env > ${_CFG_PATH}/.env
            ;;
        *)
            _usage >&2
            exit 1
            ;;
    esac
}
